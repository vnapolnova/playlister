{
  "name": "playlister",
  "productName": "Playlister",
  "version": "1.0.0",
  "description": "Compare playlists across Apple Music and YouTube Music",
  "main": ".vite/build/index.js",
  "private": true,
  "scripts": {
    "start": "electron-forge start",
    "dev": "concurrently -n \"DevServer,Electron\" -c \"cyan,green\" \"npm:dev:server\" \"npm:start\"",
    "dev:server": "tsx src/dev-server/index.ts",
    "dev:browser": "concurrently -n \"DevServer,Browser\" -c \"cyan,blue\" \"npm:dev:server\" \"echo Browser: Open http://localhost:5173\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [],
  "author": {
    "name": "Valentina Napolnova",
    "email": "vnapolnova@toryburch.com"
  },
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.21.2",
    "googleapis": "^170.1.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-vite": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.1.2",
    "electron": "^40.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "tsx": "^5.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
